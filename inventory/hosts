    - name: internal20
      subnet_cidr: 10.10.20.0/24
      subnet_name: internal20
    - external: true
      name: public
      routers:
      - interfaces:
        - internal20
        name: router20
      subnet_cidr: 10.10.10.0/24
      subnet_name: public
    osp_security_groups:
    - name: apps
      rules:
      - port: 22
      - port: 8080
    - name: appdbs
      rules:
      - port: 22
      - port: 5432
    - name: frontends
      rules:
      - port: 22
      - port: 80
      - port: 443
